{
  "name": "nabiq-web-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings=100",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\" --config ./.prettierrc",
    "preview": "vite preview",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "npm run lint",
      "npm run format"
    ]
  },
  "dependencies": {
    "@mantine/code-highlight": "^7.12.2",
    "@mantine/core": "^7.10.1",
    "@mantine/dates": "^7.10.1",
    "@mantine/dropzone": "^7.12.2",
    "@mantine/form": "^7.10.1",
    "@mantine/hooks": "^7.10.1",
    "@reduxjs/toolkit": "^2.2.5",
    "@tabler/icons-react": "^3.14.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@xyflow/react": "^12.1.1",
    "aws-amplify": "^5",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.11",
    "lodash": "^4.17.21",
    "moment-timezone": "^0.5.45",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.5",
    "react-hot-toast": "^2.4.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.23.1",
    "react-xarrows": "^2.0.2",
    "react18-input-otp": "^1.1.4",
    "recharts": "^2.12.7",
    "redux-persist": "^6.0.0",
    "unplugin-fonts": "^1.1.1",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.6",
    "@types/moment-timezone": "^0.5.30",
    "@types/node": "^22.7.2",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@types/recharts": "^1.8.29",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.10",
    "postcss": "^8.4.38",
    "postcss-preset-mantine": "^1.15.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.3.3",
    "sass": "^1.77.4",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.4.0",
    "vite": "^5.2.0"
  }
}
